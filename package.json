{
    "name": "react4xp-runtime-externals",
    "version": "0.3.1",
    "description": "React4xp helper: using webpack externals as input, generates a runnable externals chunk for shared external libraries",
    "main": "webpack.config.js",
    "files": [
        "webpack.config.js",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "clean": "rimraf ./webpack.config.js && rimraf build && rimraf src/_AUTOGENERATED_tmp_externals_.es6",
        "copy": "copyfiles --flat src/*.* .",
        "prepublish": "npm run clean && npm run copy",
        "postpublish": "npm run clean",
        "test": "node_modules/webpack-cli/bin/cli.js --config src/webpack.config.js --env.REACT4XP_CONFIG_FILE=$(pwd)/test/react4xpConstants.json --env.BUILD_R4X=$(pwd)/build && echo \"\" && echo \"--> build/\" && ls build",
        "pretest": "npm run clean"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enonic/react4xp-runtime-externals"
    },
    "author": "Espen Norderud",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/enonic/react4xp-runtime-externals/issues"
    },
    "homepage": "https://github.com/enonic/react4xp-runtime-externals/",
    "dependencies": {
        "chunks-2-json-webpack-plugin": "^1.0.0"
    },
    "peerDependencies": {
        "webpack": "^4.0.0",
        "webpack-cli": "^3.0.0",
        "babel-cli": "^6.0.0",
        "babel-loader": "^7.0.0",
        "babel-preset-es2015": "^6.0.0",
        "babel-preset-stage-0": "^6.0.0",
        "babel-register": "^6.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-loader": "^7.1.5",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "copyfiles": "^2.1.0",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "rimraf": "^2.0.0",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-0"
        ]
    }
}
